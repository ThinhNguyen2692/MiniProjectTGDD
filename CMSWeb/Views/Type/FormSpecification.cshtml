@model string;
<div class="content-wrapper">
    <div style="width: 1200px; margin: auto;" class="grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4></h4>
                <h4 class="card-title">Nhập thông số kỹ thuật SP</h4>
                
                <form class="forms-sample" id="form-add-Specification" method="post" enctype="multipart/form-data" action="/AddSpecification">

                    <div class="form-group">
                        <label for="exampleInputUsername1">Mã ngành hàng</label>
                        <input type="text" class="form-control" readonly name="TypeId" value="@Model" />
                        <div class="valid-feedback">
                        Looks good!
                    </div>
                    <div class="invalid-feedback">
                        Error...
                    </div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Tên thông số</label>
                        <input type="text" class="form-control" id="SpecificationsName" name="SpecificationsName"/>
                         <div class="valid-feedback">
                        Looks good!
                    </div>
                    <div class="invalid-feedback">
                        không được bỏ trống
                    </div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Mô tả</label>
                        <input type="text" class="form-control" name="SpecificationsDescription"/>
                    </div>
                     <button type="submit" id="btn-add-Type" class="btn btn-gradient-primary me-2">Thêm mới</button>
                    <button type="reset" class="btn btn-light">Làm mới</button>
                    
                </form>
                 <br />
                 <a class="btn btn-outline-danger" href="/Formtype">Kết thúc nhập</a>  
            </div>
            <br />

        </div>
    </div>
</div>
